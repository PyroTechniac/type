{
	"name": "@sapphire/type",
	"version": "2.1.2",
	"description": "A type detection utility for JavaScript",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"sideEffects": false,
	"homepage": "https://github.com/sapphiredev/type",
	"scripts": {
		"clean": "node scripts/clean.mjs",
		"docs": "typedoc",
		"pretest": "run-s gyp:configure gyp:build",
		"test": "jest",
		"lint": "eslint src tests --ext ts --fix",
		"build": "yarn gyp:configure && run-p ts:build gyp:build && gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "yarn build",
		"install": "node-pre-gyp install --fallback-to-build",
		"update": "yarn upgrade-interactive",
		"sversion": "standard-version",
		"ts:build": "tsc -b src",
		"gyp:rebuild": "node-pre-gyp rebuild",
		"gyp:configure": "node-pre-gyp configure",
		"gyp:build": "node-pre-gyp build",
		"gyp:package": "node-pre-gyp package",
		"gyp:prebuild": "node-pre-gyp configure install build package --fallback-to-build"
	},
	"binary": {
		"module_name": "type",
		"module_path": "./prebuild/{module_name}-{node_abi}-{platform}-{arch}-{libc}-{libc_version}/",
		"host": "https://github.com/sapphiredev/type/releases/download/",
		"remote_path": "v{version}",
		"package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}-{libc}-{libc_version}.tar.gz",
		"pkg_path": "."
	},
	"dependencies": {
		"@discordjs/node-pre-gyp": "^0.4.2",
		"nan": "^2.15.0",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@favware/npm-deprecate": "^1.0.4",
		"@sapphire/eslint-config": "^4.2.0",
		"@sapphire/prettier-config": "^1.3.0",
		"@sapphire/ts-config": "^3.3.1",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "~8.9.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"jest-circus": "^27.5.1",
		"lint-staged": "^12.3.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.5.0",
		"typedoc": "^0.22.11",
		"typedoc-plugin-mdn-links": "^1.0.5",
		"typescript": "^4.5.5"
	},
	"files": [
		"lib",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/type.git"
	},
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"@sapphire/type",
		"bot",
		"typescript",
		"ts",
		"discord",
		"sapphire",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/type/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz"
	},
	"gypfile": true,
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.1.1"
}
