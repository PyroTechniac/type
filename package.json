{
	"name": "@sapphire/type",
	"version": "1.2.0",
	"description": "A type detection utility for JavaScript",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"homepage": "https://github.com/sapphire-community/utilities/tree/main/packages/type",
	"scripts": {
		"docs": "typedoc",
		"pretest": "run-s configure build:native",
		"test": "jest",
		"lint": "eslint src tests --ext ts --fix",
		"prebuild": "yarn configure",
		"build": "run-p build:**",
		"build:native": "node-gyp build",
		"build:ts": "tsc -b src",
		"postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "yarn build",
		"configure": "node-gyp configure",
		"install": "node-gyp configure && node-gyp rebuild"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"nan": "^2.14.2",
		"node-gyp": "^8.0.0",
		"tslib": "^2.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.1",
		"@commitlint/config-conventional": "^12.1.1",
		"@sapphire/eslint-config": "^3.1.1",
		"@sapphire/prettier-config": "^1.1.0",
		"@sapphire/ts-config": "^2.2.0",
		"@types/bindings": "^1.5.0",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"gen-esm-wrapper": "^1.1.1",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"lint-staged": "^10.5.4",
		"npm-run-all": "^4.1.5",
		"pretty-quick": "^3.1.0",
		"standard-version": "^9.2.0",
		"ts-jest": "^26.5.5",
		"ts-node": "^9.1.1",
		"typedoc": "^0.20.36",
		"typedoc-plugin-nojekyll": "^1.0.1",
		"typescript": "^4.2.4"
	},
	"files": [
		"lib",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphire-community/utilities.git",
		"directory": "packages/type"
	},
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"@sapphire/type",
		"bot",
		"typescript",
		"ts",
		"discord",
		"sapphire",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/sapphire-community/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"gypfile": true,
	"prettier": "@sapphire/prettier-config"
}
