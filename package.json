{
	"name": "@sapphire/type",
	"version": "2.0.0",
	"description": "A type detection utility for JavaScript",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"homepage": "https://github.com/sapphiredev/utilities/tree/main/packages/type",
	"scripts": {
		"clean": "node scripts/clean.mjs",
		"docs": "typedoc",
		"pretest": "run-s gyp:configure gyp:build",
		"test": "jest",
		"lint": "eslint src tests --ext ts --fix",
		"prebuild": "run-s gyp:configure",
		"build": "run-p ts:build gyp:build",
		"postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "yarn build",
		"install": "node-pre-gyp install --fallback-to-build",
		"update": "yarn upgrade-interactive",
		"sversion": "standard-version",
		"ts:build": "tsc -b src",
		"gyp:rebuild": "node-pre-gyp rebuild",
		"gyp:configure": "node-pre-gyp configure",
		"gyp:build": "node-pre-gyp build",
		"gyp:package": "node-pre-gyp package",
		"gyp:prebuild": "node-pre-gyp install build package"
	},
	"binary": {
		"module_name": "type",
		"module_path": "./prebuild/{module_name}-{node_abi}-{platform}-{arch}-{libc}/",
		"host": "https://github.com/sapphiredev/type/releases/download/",
		"remote_path": "v{version}",
		"package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}-{libc}.tar.gz",
		"pkg_path": "."
	},
	"dependencies": {
		"@discordjs/node-pre-gyp": "^0.3.2",
		"nan": "^2.14.2",
		"tslib": "^2.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.1",
		"@commitlint/config-conventional": "^12.1.1",
		"@sapphire/eslint-config": "^3.1.2",
		"@sapphire/prettier-config": "^1.1.1",
		"@sapphire/ts-config": "^2.2.1",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"gen-esm-wrapper": "^1.1.1",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"lint-staged": "^10.5.4",
		"node-gyp": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"pretty-quick": "^3.1.0",
		"standard-version": "^9.2.0",
		"ts-jest": "^26.5.5",
		"ts-node": "^9.1.1",
		"typedoc": "^0.20.36",
		"typedoc-plugin-nojekyll": "^1.0.1",
		"typescript": "^4.2.4"
	},
	"files": [
		"lib",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/utilities.git",
		"directory": "packages/type"
	},
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"@sapphire/type",
		"bot",
		"typescript",
		"ts",
		"discord",
		"sapphire",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"gypfile": true,
	"prettier": "@sapphire/prettier-config"
}
